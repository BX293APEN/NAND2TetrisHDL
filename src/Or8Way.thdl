// Or8Way CHIP
CHIP Or8Way {
    IN in[8];
    OUT out;
    PARTS:
        Or(a=in[0], b=in[7], out=w0);
        Or(a=in[1], b=in[6], out=w1);
        Or(a=in[2], b=in[5], out=w2);
        Or(a=in[3], b=in[4], out=w3);
        Or(a=w0, b=w3, out=w4);
        Or(a=w1, b=w2, out=w5);
        Or(a=w4, b=w5, out=w6);
        Or(a=w6, b=w6, out=out);
}